<%= render "breadcrumbs", breadcrumbs: [], active: @dialogue.title %>

<h2><%= @dialogue.title %></h2>
<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="well">
      <strong>Message</strong>
      <p><%=@dialogue.message%></p>
      <strong>Yes Text</strong>
      <p><%=@dialogue.yes_text%></p>
      <strong>No Text</strong>
      <p><%=@dialogue.no_text%></p>
    </div>
  </div>
</div>